<%- include('../partials/header') %>
<main>
<div>
    <form action="/meals/<%=id%>?_method=PUT" method="POST">
        <label>Breakfast:</label>
        <input name="breakfast" value="<%=entry.breakfast %>">
        <label>Lunch:</label>
        <input name="lunch" value="<%=entry.lunch%>">
        <label>Dinner:</label>
        <input name="dinner" value="<%=entry.dinner%>">
        <label>Other:</label>
        <input name="other" value="<%=entry.other%>">        
        <label>Date:</label>
        <input name="date" value="<%=entry.date%>" type="date" >
        <input type="submit" value="log">
    </form>
</div>
</main>
</body>
</html>